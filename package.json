{
    "name": "gustavo",
    "version": "0.0.1",
    "description": "track spending and split costs",
    "author": "Ivan Wong",
    "repository": "git+https://github.com/iwong3/gustavo.git",
    "scripts": {
        "build": "react-scripts build",
        "deploy": "gh-pages -d build",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "start": "react-scripts start",
        "test": "react-scripts test",
        "db:install": "powershell -ExecutionPolicy Bypass -File scripts/install-postgres-windows.ps1",
        "db:install-bash": "bash scripts/install-postgres-windows.sh",
        "db:schema-local": "psql -U gustavo_user -d gustavo_dev -f database/schema.sql",
        "db:test-local": "psql -U gustavo_user -d gustavo_dev -c \"SELECT version();\"",
        "db:reset": "powershell -ExecutionPolicy Bypass -File scripts/reset-database.ps1",
        "db:reset-bash": "bash scripts/reset-database.sh",
        "db:reset-force": "powershell -ExecutionPolicy Bypass -File scripts/reset-database.ps1 -Force",
        "env:create": "powershell -ExecutionPolicy Bypass -File scripts/create-env-local.ps1",
        "env:create-bash": "bash scripts/create-env-local.sh"
    },
    "dependencies": {
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@mui/material": "^6.1.1",
        "@phosphor-icons/react": "^2.1.7",
        "@tabler/icons-react": "^3.19.0",
        "@visx/annotation": "^3.3.0",
        "@visx/axis": "^3.10.1",
        "@visx/brush": "^3.10.4",
        "@visx/group": "^3.3.0",
        "@visx/scale": "^3.5.0",
        "@visx/shape": "^3.5.0",
        "axios": "^1.7.7",
        "dayjs": "^1.11.13",
        "fuse.js": "^7.0.0",
        "pg": "^8.11.3",
        "react": "^18.2.0",
        "react-animate-height": "^3.2.3",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.26.2",
        "react-scripts": "5.0.1",
        "zustand": "^5.0.0-rc.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^28.1.6",
        "@types/node": "^18.6.3",
        "@types/pg": "^8.10.9",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "gh-pages": "^6.1.1",
        "jest": "^28.1.3",
        "ts-jest": "^28.0.8",
        "typescript": "^4.7.4"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
