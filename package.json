        "@tabler/icons-react": "^3.19.0",
        "@visx/annotation": "^3.3.0",
        "@visx/axis": "^3.10.1",
        "@visx/brush": "^3.10.4",
        "@visx/group": "^3.3.0",
        "@visx/scale": "^3.5.0",
        "@visx/shape": "^3.5.0",
        "axios": "^1.7.7",
        "dayjs": "^1.11.13",
        "fuse.js": "^7.0.0",
        "next": "^14.0.0",
        "next-pwa": "^5.6.0",
        "pg": "^8.11.3",
        "react": "^18.2.0",
