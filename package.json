        "eslint-config-next": "^14.0.0",
        "gh-pages": "^6.1.1",
        "typescript": "^5.0.0",
        "vercel": "^37.4.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
