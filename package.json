{
    "name": "gustavo",
    "version": "0.0.1",
    "description": "track spending and split costs",
    "author": "Ivan Wong",
    "repository": "git+https://github.com/iwong3/gustavo.git",
    "scripts": {
        "start": "yarn docker:db && echo 'Starting full development environment...' && concurrently \"yarn backend\" \"yarn react\"",
        "react": "vite",
        "backend": "cd backend && ts-node server.ts",
        "docker:db": "docker-compose -f infra/docker-compose.yml up -d postgres",
        "stop": "docker-compose -f infra/docker-compose.yml down && taskkill /F /IM node.exe 2>nul || true",
        "db:reset": "bash scripts/reset-database.sh",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "deploy": "vercel --prod"
    },
    "dependencies": {
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@mui/material": "^6.1.1",
        "@phosphor-icons/react": "^2.1.7",
        "@tabler/icons-react": "^3.19.0",
        "@visx/annotation": "^3.3.0",
        "@visx/axis": "^3.10.1",
        "@visx/brush": "^3.10.4",
        "@visx/group": "^3.3.0",
        "@visx/scale": "^3.5.0",
        "@visx/shape": "^3.5.0",
        "axios": "^1.7.7",
        "dayjs": "^1.11.13",
        "express": "^4.18.2",
        "fuse.js": "^7.0.0",
        "pg": "^8.11.3",
        "react": "^18.2.0",
        "react-animate-height": "^3.2.3",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.26.2",
        "zustand": "^5.0.0-rc.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/express": "^4.17.22",
        "@types/node": "^18.19.110",
        "@types/pg": "^8.10.9",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@vitejs/plugin-react": "^4.5.0",
        "@vitest/ui": "^3.1.4",
        "concurrently": "^8.2.2",
        "gh-pages": "^6.1.1",
        "jsdom": "^26.1.0",
        "ts-node": "^10.9.2",
        "typescript": "^4.7.4",
        "vercel": "^37.4.0",
        "vite": "^6.3.5",
        "vitest": "^3.1.4"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
